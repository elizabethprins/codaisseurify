<%= form_for @artist do |f| %>

  <ul>
     <% @artist.errors.full_messages.each do |msg| %>
       <li><%= msg %></li>
     <% end %>
  </ul>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :img_url %>
  <%= f.text_field :img_url %>

  <%= f.submit ("Add this artist!") %>

<% end %>
